@model ArticleType
@{
    ViewBag.Title = Model.Name;
    ViewBag.Breadcrumb = new BreadcrumbModel { Items = new List<BreadcrumbItem> { new BreadcrumbItem { Text = Model.Name, Url = null, IsLast = true } } };
}
@Html.Action("ViewByType", "Article", new { typeId = Model.Id, viewPath = "Catalogue/_List", sortOrder = ArticleSortOrder.SORT_ORDER, direction = VCMS.MVC4.Data.SortDirection.DESCENDING })
