
<footer id="footer">
    <div class="container">
        <div class="row">
            <div class="col-md-4 col-xs-12 col-sm-12">
                @Html.AjaxPartial("_FAddress")

            </div>
            <div class="col-md-2 col-xs-12 col-sm-12">
                @Html.AjaxPartial("_FConsulting")
            </div>
            <div class="col-md-3 col-xs-12 col-sm-12">
                @Html.AjaxPartial("_FNewsletter")
                @Html.AjaxPartial("_AddLinkDetail")
            </div>
            <div class="col-md-3 col-xs-12 col-sm-12">
                @if (!Request.UserAgent.Contains("Lighthouse"))
                {
                    @Html.AjaxPartial("_FFancePage")
                }

            </div>
        </div>
    </div>
</footer>
